{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Using for ms1",
			"command": "/usr/bin/g++",
			"args": [
				"-fdiagnostics-color=always",
				"-Wall",
				"-std=c++11",
				"-g",
				"${fileDirname}/Status*.cpp",
				"-o",
				"main"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/g++"
		}
	]
}